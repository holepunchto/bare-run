{
  "name": "bare-run",
  "version": "0.0.3",
  "description": "Cross-platform script runner for Bare",
  "exports": {
    "./package": "./package.json",
    ".": "./index.js"
  },
  "imports": {
    "child_process": {
      "bare": "bare-subprocess",
      "default": "child_process"
    },
    "fs": {
      "bare": "bare-fs",
      "default": "fs"
    },
    "os": {
      "bare": "bare-os",
      "default": "os"
    },
    "path": {
      "bare": "bare-path",
      "default": "path"
    },
    "process": {
      "bare": "bare-process",
      "default": "process"
    },
    "url": {
      "bare": "bare-url",
      "default": "url"
    }
  },
  "bin": {
    "bare-run": "bin.js"
  },
  "files": [
    "index.js",
    "bin.js",
    "CMakeLists.txt",
    "lib",
    "prebuilds"
  ],
  "scripts": {
    "test": "prettier . --check && bare test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holepunchto/bare-run.git"
  },
  "author": "Holepunch",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/holepunchto/bare-run/issues"
  },
  "homepage": "https://github.com/holepunchto/bare-run#readme",
  "engines": {
    "bare": ">=1.16.0"
  },
  "dependencies": {
    "bare-bundle": "^1.9.0",
    "bare-bundle-id": "^1.0.1",
    "bare-fs": "^4.2.1",
    "bare-module": "^5.0.2",
    "bare-module-traverse": "^1.5.1",
    "bare-os": "^3.6.2",
    "bare-pack": "^1.4.2",
    "bare-path": "^3.0.0",
    "bare-process": "^4.2.1",
    "bare-subprocess": "^5.1.1",
    "bare-unpack": "^1.0.2",
    "bare-url": "^2.2.2",
    "bare-which": "^2.0.0",
    "paparam": "^1.8.0",
    "require-asset": "^1.1.0"
  },
  "devDependencies": {
    "brittle": "^3.18.0",
    "cmake-bare": "^1.6.5",
    "cmake-bare-bundle": "^2.1.4",
    "cmake-fetch": "^1.4.0",
    "prettier": "^3.4.1",
    "prettier-config-standard": "^7.0.0"
  }
}
